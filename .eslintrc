{
  "extends": [
    "standard",
  ],
  "plugins": [
  ],
  "env": {
    "browser" : true
  },
  "rules": {
    "key-spacing"          : 0,
    "jsx-quotes"           : [2, "prefer-single"],
    "max-len"              : [2, 120, 2],
    "object-curly-spacing" : [2, "always"],
    "comma-dangle"         : [0, "always-multiline"],
  }
}
